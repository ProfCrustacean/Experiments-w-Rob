{
  "name": "experiments-w-rob",
  "version": "0.1.0",
  "description": "Catalog enrichment pipeline for school supply matching",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:migrate": "tsx src/cli/migrate.ts",
    "pipeline": "tsx src/cli/pipeline.ts",
    "pipeline:env": "tsx src/cli/pipeline-from-env.ts",
    "canary": "tsx src/cli/canary-run.ts",
    "qa:evaluate": "tsx src/cli/qa-evaluate.ts",
    "qa:import": "tsx src/cli/qa-import.ts",
    "logs:run": "tsx src/cli/logs-run.ts",
    "logs:download": "tsx src/cli/logs-download.ts",
    "report:download": "tsx src/cli/report-download.ts",
    "report:list": "tsx src/cli/report-list.ts",
    "self-improve:enqueue": "tsx src/cli/self-improve-enqueue.ts",
    "self-improve:worker": "tsx src/cli/self-improve-worker.ts",
    "self-improve:status": "tsx src/cli/self-improve-status.ts",
    "self-improve:cancel": "tsx src/cli/self-improve-cancel.ts",
    "harness:build": "tsx src/cli/harness-build.ts",
    "harness:eval": "tsx src/cli/harness-eval.ts",
    "learn:propose": "tsx src/cli/learn-propose.ts",
    "learn:apply": "tsx src/cli/learn-apply.ts",
    "learn:rollback": "tsx src/cli/learn-rollback.ts",
    "docs:check": "tsx src/cli/docs-check.ts --report-path outputs/docs_health_report.md",
    "docs:scoreboard": "tsx src/cli/docs-scoreboard.ts --window-days 7 --output docs/quality-scoreboard.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProfCrustacean/Experiments-w-Rob.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ProfCrustacean/Experiments-w-Rob/issues"
  },
  "homepage": "https://github.com/ProfCrustacean/Experiments-w-Rob#readme",
  "dependencies": {
    "csv-parse": "^6.1.0",
    "dotenv": "^17.3.1",
    "openai": "^6.22.0",
    "p-limit": "^7.3.0",
    "pg": "^8.18.0",
    "slugify": "^1.6.6",
    "xlsx": "^0.18.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.0"
  }
}
